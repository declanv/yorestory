<nav class= "nav">
          <p>

        <%= link_to "users", users_path %> | <%= link_to "timelines", timelines_path  %> | <%= link_to "logout", "user/logout"  %><br>
        <%= " Welcome, #{current_user.name}" if current_user %>
      </p>
        </nav>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <h4 class="user_name"><%= link_to user.name, "/users/#{user.id}" %></h4>

<p class="user_bio">
  <%= user.bio %>
</p>
<img class="user_photo" src=<%= user.photo %>>
      <p>
        <ul class="timelines">
        <% user.timelines.each do |timeline| %>
          <li class="timeline_title">
            <%= link_to timeline.title, "/timelines/#{timeline.id}" %>
          </li>
        <% end %>
        </ul>
      </p>
    </li>
  <% end %>
</ul>
